<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="UClientMessageDao">
	<insert id="ceate" parameterType="UClientMessage">
		insert into u_client_message
		(lordId,version,plat,definition,msgData,createDate)	values
		(#{lordId},#{version},#{plat},#{definition},#{msgData},#{createDate})
	</insert>
</mapper>